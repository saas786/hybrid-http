{
    "name": "themehybrid/hybrid-http",
    "description": "The Http package.",
    "license": "MIT",
    "homepage": "https://laravel.com",
    "support": {
        "issues": "https://github.com/themehybrid/hybrid-http/issues",
        "source": "https://github.com/themehybrid/hybrid-http"
    },
    "authors": [
        {
            "name": "Taylor Otwell",
            "email": "taylor@laravel.com"
        }
    ],
    "require": {
        "php" : "^7.4|^8.0.2",
        "ext-json": "*",
        "fruitcake/php-cors": "^1.2",
        "symfony/http-foundation": "^6.0",
        "symfony/http-kernel": "^6.0",
        "symfony/mime": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Hybrid\\Http\\": ""
        }
    },
    "suggest": {
        "ext-gd": "Required to use Illuminate\\Http\\Testing\\FileFactory::image().",
        "guzzlehttp/guzzle": "Required to use the HTTP Client (^7.2)."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "9.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
